{
    "name": "@tihlde/sdk",
    "version": "0.0.2",
    "description": "TypeScript SDK for the Photon API",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./auth": {
            "types": "./dist/auth.d.ts",
            "default": "./dist/auth.js"
        }
    },
    "files": ["dist", "scripts"],
    "scripts": {
        "build": "bun build.ts",
        "codegen": "openapi-ts",
        "typecheck": "tsc --noEmit",
        "postinstall": "node scripts/postinstall.mjs"
    },
    "dependencies": {
        "@hey-api/client-fetch": "^0.13.1"
    },
    "devDependencies": {
        "@hey-api/openapi-ts": "0.87.5",
        "@photon/auth": "workspace:*",
        "@photon/tsconfig": "workspace:*",
        "@types/bun": "catalog:",
        "@types/node": "catalog:",
        "better-auth": "catalog:",
        "typescript": "catalog:"
    },
    "peerDependencies": {
        "better-auth": "^1.4.0"
    },
    "peerDependenciesMeta": {
        "better-auth": {
            "optional": true
        }
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TIHLDE/Photon"
    }
}
